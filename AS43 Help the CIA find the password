#Help the CIA find the password
text = "Hello there."
maximumletter = text[0]
minimumletter = text[0]
max = text.count(text[0])
min = text.count(text[0])
for i in text:
    if i is not " ":
        if text.count(i) > max:
            maximumletter = i
            max = text.count(i)
        if text.count(i) < min:
            minimumletter = i
            min = text.count(i)

print (max, maximumletter)
print (min, minimumletter)
